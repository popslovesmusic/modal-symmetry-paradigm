  // ------------------------------------------------------------
  // Naming Rules and Patterns
  // ------------------------------------------------------------

  naming_rules: {
    operators: {
      description: "Canonical naming for operators across tiers.",
      pattern: "OP_<TIER>_<FAMILY>_<NAME>",
      // pattern examples use UPPER_SNAKE and two-digit tier index.
      examples: [
        "OP_T01_DELTA_GRADIENT",
        "OP_T02_PHI_PROJECT_TO_SEMANTIC_FIELD",
        "OP_T09_CHI_STEP_TIME_EULER"
      ],
      constraints: [
        "TIER component uses T00–T12 with leading zero.",
        "FAMILY uses the Latinized name of the symbol (DELTA, PHI, PI, MU, LAMBDA, PSI, SIGMA, THETA, CHI, OMEGA, RHO, XI).",
        "NAME must be descriptive of function, using UPPER_SNAKE."
      ]
    },
    axioms: {
      description: "Axiom identifiers within each tier.",
      pattern: "AX_T<TIER>_<SHORT_NAME>",
      examples: [
        "AX_T00_NO_PERFECT_ZERO",
        "AX_T01_DEVIATION_IS_NONNEGATIVE",
        "AX_T03_TRIUNITY_EVALUATION_CONSISTENCY"
      ]
    },
    invariants: {
      description: "Invariant identifiers (global or tier-scoped).",
      pattern: "INVAR_<SCOPE>_<SHORT_NAME>",
      examples: [
        "INVAR_GLOBAL_TRIUNITY_CONSISTENCY",
        "INVAR_T11_LAYER_IDENTITY",
        "INVAR_T09_FORWARD_FLOW"
      ],
      scope_values: ["GLOBAL", "T00", "T01", "…", "T12"]
    },
    tests: {
      description: "Test and regression identifiers.",
      pattern: "TEST_<TIER>_<CATEGORY>_<NAME>",
      examples: [
        "TEST_T00_PRIMITIVE_STRUCTURES_SMOKE",
        "TEST_T11_RHO_LAYER_INVARIANTS",
        "TEST_T12_XI_SCHEMA_COMPATIBILITY"
      ]
    },
    files: {
      description: "Tier file naming for JSON5 artifacts.",
      tier_files: [
        "tier_XX_metadata.json5",
        "tier_XX_operator_pack.json5",
        "tier_XX_interaction_table.json5",
        "tier_XX_axiom_box.json5",
        "tier_XX_rewrite_system.json5",
        "tier_XX_module_pack.json5",
        "tier_XX_test_suite.json5"
      ],
      notes: "XX is a two-digit tier index from 00 to 12."
    }
  },

  // ------------------------------------------------------------
  // Governance: Change, Deprecation, and Review
  // ------------------------------------------------------------

  governance: {
    versioning: {
      scheme: "semantic",
      format: "MAJOR.MINOR.PATCH",
      rules: {
        MAJOR: "Breaking changes to nomenclature or symbol semantics.",
        MINOR: "New terms, new symbols, or non-breaking refinements.",
        PATCH: "Typo fixes, clarifications, or documentation updates."
      }
    },
    change_process: {
      levels: [
        {
          id: "trivial",
          description: "Typos, formatting, or strictly clarifying edits.",
          requires_review: false
        },
        {
          id: "minor",
          description: "New terms, examples, or aliases that do not change existing semantics.",
          requires_review: true
        },
        {
          id: "breaking",
          description: "Renaming symbols, changing definitions, or altering operator families.",
          requires_review: true,
          requires_migration_plan: true
        }
      ],
      workflow: [
        "Propose: Change drafted as a patch to this JSON5 and MD documents.",
        "Review: Human + Librarian agent verify consistency across tiers and schemas.",
        "Approve: Update version and changelog.",
        "Migrate: Apply renames/updates to source files and agents.",
        "Publish: Tag release and notify consuming agents."
      ]
    },
    deprecation: {
      policy: "No immediate deletion of terms or symbols; deprecate first.",
      fields: [
        "status: active | deprecated",
        "deprecated_in_version",
        "replacement_term"
      ]
    },
    review_cycle: {
      frequency: "P3M", // every 3 months
      description: "Full nomenclature review and reconciliation against the live library."
    }
  }
}
