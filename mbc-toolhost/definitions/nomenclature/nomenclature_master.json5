    allowed_characters: {
      identifiers: "A–Z, a–z, 0–9, underscore (_)",
      symbols: "Unicode math / Greek where explicitly registered (δ, Φ, Π, μ, λ, ψ, Σ, Θ, χ, Ω, ρ, Ξ, etc.)"
